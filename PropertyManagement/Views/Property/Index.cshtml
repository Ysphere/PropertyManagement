@model List<Property>
@{
    ViewData["Title"] = "Shit Page";
}
<div class= "container">
    <div class= "row pt-4 ">
        <div class="col-6">
            <h2 class="text-primary">Fuck</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller =""
        </div>

    </div>
    <table class=" table table-bordered table-hover">
        <thead>
            <tr>
                <th>Address</th>
                <th>Property Type</th>
                <th>Bought Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var property in Model)
            {
                <tr>
                    <td>d</td>
                    <td>@property.PropertyType.ToString()</td>
                    <td>@property.BoughtDate.Value.ToLocalTime().ToString()</td>
                </tr>
            }
        </tbody>
    </table>
 </div>